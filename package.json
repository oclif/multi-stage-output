    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19",
    "@eslint/compat": "^1.2.8",
    "@oclif/prettier-config": "^0.2.1",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18",
    "@types/sinon": "^17.0.3",
    "chai": "^4.5.0",
    "commitlint": "^19",
    "eslint": "^9.25.1",
    "eslint-config-oclif": "^6.0.47",
    "eslint-config-prettier": "^10",
    "eslint-config-xo": "^0.47.0",
    "eslint-config-xo-react": "^0.28.0",
    "eslint-plugin-react": "^7.37.5",
